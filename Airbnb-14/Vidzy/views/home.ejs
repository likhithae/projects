<%- include('partials/header') %>

    <div class="container-fluid m-0" style="padding-top:70px;">
        <%if (message ){%>
            <div class="alert alert-warning search-msg" ><%=message%>
            </div>
        <%}%>
        <%- include ('partials/search_bar')%>

    </div>

    <div id="main">
        <div class="container m-2"></div>

        <div id="carouselExampleCaptions" class="carousel slide m-3" data-bs-ride="false">
            <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
                    aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                    aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                    aria-label="Slide 3"></button>
            </div>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="images/img1.jpeg" class="d-block w-100 h-20" alt="Image 1">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>A vacation is having nothing to do and all day to do it in.</h5>
                        <p>-Robert Orben</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="images/img2.jpeg" class="d-block w-100" alt="Image 2">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>A vacation spot our of season always has a very special magic.</h5>
                        <p>– Max van Sydow</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="images/img3.webp" class="d-block w-100" alt="Image 3">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Here’s to a vacation of no regrets!</h5>
                        <p>– Joan Rylen</p>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>

        <div class="bg-light p-5 rounded-lg m-5 jumbotroncustom" style="margin-left: 3cm;">
            <div class="row">
                <div class="col col-md-5">
                    <h5>There’s no place like home, except for the Cozy Crib. Live in the elegant comfort we have in
                        store for you!</h5><br><br>
                    <a class="btn btn-primary btn-lg" href="#" role="button">Learn more</a>
                </div>
                <div class="col col-md-1"></div>
                <div class="col col-md-2"><svg xmlns="http://www.w3.org/2000/svg" width="70" height="70" fill="orange"
                        class="bi bi-people-fill" viewBox="0 0 16 16">
                        <path d="M7 14s-1 0-1-1 1-4 5-4 5 3 5 4-1 1-1 1H7zm4-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                        <path fill-rule="evenodd"
                            d="M5.216 14A2.238 2.238 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.325 6.325 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1h4.216z" />
                        <path d="M4.5 8a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z" />
                    </svg><br>500+ Guests</div>
                <div class="col col-md-2"><svg xmlns="http://www.w3.org/2000/svg" width="70" height="70" fill="orange"
                        class="bi bi-person-fill" viewBox="0 0 16 16">
                        <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                    </svg><br>75+ Hosts</div>
                <div class="col col-md-2"><svg xmlns="http://www.w3.org/2000/svg" width="70" height="70" fill="orange"
                        class="bi bi-house-heart-fill" viewBox="0 0 16 16">
                        <path
                            d="M7.293 1.5a1 1 0 0 1 1.414 0L11 3.793V2.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v3.293l2.354 2.353a.5.5 0 0 1-.708.707L8 2.207 1.354 8.853a.5.5 0 1 1-.708-.707L7.293 1.5Z" />
                        <path
                            d="m14 9.293-6-6-6 6V13.5A1.5 1.5 0 0 0 3.5 15h9a1.5 1.5 0 0 0 1.5-1.5V9.293Zm-6-.811c1.664-1.673 5.825 1.254 0 5.018-5.825-3.764-1.664-6.691 0-5.018Z" />
                    </svg><br>250+ Properties</div>

            </div>
        </div>

    </div>

    <div class="container mb-5 mt-5 property-list">
        <%- include ('partials/list_properties')%>
    </div>
